{
  "name": "react-dat-gui",
  "version": "3.0.0",
  "description": "dat.GUI reimagined for React",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "style": "dist/index.css",
  "types": "index.d.ts",
  "contributors": [
    {
      "name": "Claus Wahlers",
      "email": "claus@codeazur.com.br",
      "url": "https://github.com/claus"
    },
    {
      "name": "Rohan Deshpande",
      "email": "rohan@creativelifeform.com",
      "url": "https://github.com/rohan-deshpande"
    }
  ],
  "license": "MIT",
  "keywords": [
    "react",
    "dat.gui"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:claus/react-dat-gui.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "dev": "npm run start --prefix dev",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix-js": "yarn run lint-js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{js, jsx}": [
      "prettier --write",
      "eslint --no-ignore --fix",
      "git add --force"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "classnames": "2.2.6",
    "lodash.clamp": "^4.0.2",
    "lodash.clonedeep": "^4.3.2",
    "lodash.isfinite": "^3.3.1",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.result": "^4.4.0",
    "lodash.set": "^4.2.0",
    "react-color": "^2.13.8"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^1.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
